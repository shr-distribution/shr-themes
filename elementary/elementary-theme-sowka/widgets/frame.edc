group { name: "elm/frame/base/default";
   images {
      image: "dia_grad.png" COMP;
   }
   parts {
      part { name: "base0";
         mouse_events:  0;
         description { state: "default" 0.0;
            image.normal: "dia_grad.png";
            image.border: 9 9 9 9;
            rel1.offset: 4 4;
            rel2.to: "elm.swallow.content";
            rel2.offset: 5 5;
         }
      }
      part { name: "elm.text";
         type: TEXT;
         mouse_events:   0;
         scale: 1;
         description { state: "default" 0.0;
            align: 0.0 0.0;
            fixed: 0 1;
            rel1 {
               relative: 0.0 0.0;
               offset: 11 8;
            }
            rel2 {
               relative: 1.0 0.0;
               offset: -10 6;
            }
            color: 0 0 0 255;
            text {
               font: "Sans:style=Bold,Edje-Vera-Bold";
               size: 10;
               min: 1 1;
               max: 1 1;
               align: 0.0 0.0;
            }
         }
      }
      part { name: "elm.swallow.content";
         type: SWALLOW;
         description { state: "default" 0.0;
            align: 0.0 0.0;
            rel1 {
               to_y: "elm.text";
               relative: 0.0 1.0;
               offset: 10 2;
            }
            rel2.offset: -12 -12;
         }
      }
   }
}

