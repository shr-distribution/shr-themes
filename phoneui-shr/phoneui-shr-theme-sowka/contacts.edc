				rel2.relative: 1.0 0.9;
			}
			description {
				state: "edit" 0.0;
				inherit: "default" 0.0;
				visible: 1;
			}
		}
		part { name: "elm.swallow.button_actions";
			type: SWALLOW;
			description {
				state: "default" 0.0;
				visible: 1;
				fixed: 1 1;
				align: 1 0;
				rel1 {
					to: "elm.swallow.button_delfield";
					relative: 0.0 0.0;
				}
				rel2 {
					to: "elm.swallow.button_delfield";
