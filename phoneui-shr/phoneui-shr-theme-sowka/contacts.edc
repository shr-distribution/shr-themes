group { name: "phoneui/contacts/list";
	parts {
		part { name: "contacts_list";
			type: SWALLOW;
			mouse_events: 1;
			description {
				state: "default" 0.0;
				rel1 {
					relative: 0.0 0.0;
					offset: 0 0;
				}
				rel2 {
					relative: 1.0 0.85;
					offset: 0 0;
				}
			}
		}
		part { name: "contacts_index";
			type: SWALLOW;
			mouse_events: 1;
			description {
				fixed: 1 1;
				state: "default" 0.0;
				rel1 {
					to_y: "contacts_list";
					relative: 0.0 0.0;
					offset: 0 0;
				}
				rel2 {
					to_y: "contacts_list";
					relative: 1.0 1.0;
					offset: 0 0;
				}
			}
		}
		part { name: "button_new";
			type: SWALLOW;
			mouse_events: 1;
			description {
				state: "default" 0.0;
				rel1 {
					relative: 0.0 0.85;
				}
				rel2 {
					relative: 0.33 1.0;
				}
			}
		}
		part { name: "button_call";
			type: SWALLOW;
			mouse_events: 1;
			description {
				state: "default" 0.0;
				rel1 {
					relative: 0.33 0.85;
				}
				rel2 {
					relative: 0.66 1.0;
				}
			}
		}
		part { name: "button_options";
			type: SWALLOW;
			mouse_events: 1;
			description {
				state: "default" 0.0;
				rel1 {
					relative: 0.66 0.85;
				}
				rel2 {
					relative: 1.0 1.0;
				}
			}
		}
		part { name: "loading_clipper";
			type: RECT;
			repeat_events: 1;
			description {
				state: "default" 0.0;
				visible: 0;
				color: 255 255 255 0;
			}
			description {
				state: "loading" 0.0;
				visible: 1;
				color: 255 255 255 255;
			}
		}
		part { name: "loading_bg";
			mouse_events: 1;
			type: RECT;
			clip_to: "loading_clipper";
			description {
				state: "default" 0.0;
				color: 0 0 0 100;
				rel1.relative: 0.0 0.75;
				rel2.relative: 1.0 0.85;
			}
		}
		part { name: "loading_indicator";
			type: SWALLOW;
			mouse_events: 0;
			clip_to: "loading_clipper";
			description {
				state: "default" 0.0;
				fixed: 1 1;
				rel1 {
					to: "loading_bg";
					relative: 1.0 1.0;
				}
				rel2 {
					to: "loading_bg";
					relative: 0.0 0.0;
				}
			}
		}
	}
	programs {
		program {
			name:   "start_loading";
			signal:  "start_loading";
			source:  "";
			action:  STATE_SET "loading" 0.0;
			target:  "loading_clipper";
			transition: ACCELERATE 0.4;
		}
		program {
			name:   "stop_loading";
			signal:  "stop_loading";
			source:  "";
			action:  STATE_SET "default" 0.0;
			target:  "loading_clipper";
			transition: DECELERATE 0.4;
		}
	}
} /* phoneui/contacts/list */

